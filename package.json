{
  "name": "gaming",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "buefy": "^0.8.10",
    "core-js": "^3.6.4",
    "mongodb": "^3.5.2",
    "mongodb-stitch-browser-sdk": "^4.8.0",
    "mongodb-stitch-browser-services-aws": "^4.8.0",
    "vue": "^2.6.11",
    "vue-form-generator": "^2.3.4",
    "vue-js-modal": "^1.3.31",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^2.7.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-vuex": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/attribute-hyphenation": 1,
      "vue/html-closing-bracket-newline": 1,
      "vue/html-closing-bracket-spacing": 1,
      "vue/html-end-tags": 1,
      "vue/html-indent": 1,
      "vue/html-quotes": 1,
      "vue/html-self-closing": 1,
      "vue/max-attributes-per-line": 1,
      "vue/multiline-html-element-content-newline": 1,
      "vue/mustache-interpolation-spacing": 1,
      "vue/name-property-casing": 1,
      "vue/no-multi-spaces": 1,
      "vue/no-spaces-around-equal-signs-in-attribute": 1,
      "vue/no-template-shadow": 1,
      "vue/prop-name-casing": 1,
      "vue/require-default-prop": 1,
      "vue/require-prop-types": 1,
      "vue/singleline-html-element-content-newline": 1,
      "vue/v-bind-style": 1,
      "vue/v-on-style": 1,
      "vue/attributes-order": 1,
      "vue/no-confusing-v-for-v-if": 1,
      "vue/no-v-html": 1,
      "vue/order-in-components": 1,
      "vue/this-in-template": 1,
      "vue/array-bracket-spacing": 1,
      "vue/arrow-spacing": 1,
      "vue/block-spacing": 1,
      "vue/brace-style": 1,
      "vue/camelcase": 1,
      "vue/comma-dangle": 1,
      "vue/component-name-in-template-casing": 1,
      "vue/eqeqeq": 1,
      "vue/key-spacing": 1,
      "vue/match-component-file-name": 1,
      "vue/no-boolean-default": 1,
      "vue/no-restricted-syntax": 1,
      "vue/object-curly-spacing": 1,
      "vue/require-direct-export": 1,
      "vue/script-indent": 1,
      "vue/space-infix-ops": 1,
      "vue/space-unary-ops": 1,
      "vue/v-on-function-call": 1,
      "vue/no-template-key": 1,
      "vue/no-unused-components": 1,
      "vue/no-unused-vars": 1,
      "vue/require-v-for-key": 1,
      "vue/no-use-v-if-with-v-for": 1,
      "vue/require-component-is": 1,
      "vue/require-prop-type-constructor": 1,
      "vue/require-render-return": 1,
      "vue/require-valid-default-prop": 1,
      "vue/return-in-computed-property": 1
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
